; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Tappie"
run/main_scene="res://Scenes/Home.tscn"
config/features=PackedStringArray("4.2", "C#", "Mobile")
config/icon="res://icon.svg"

[autoload]

EventManager="*res://Managers/EventManager.cs"
SceneManager="*res://Managers/SceneManager.cs"
GameManager="*res://Managers/GameManager.cs"

[display]

window/size/viewport_width=1080
window/size/viewport_height=1920
window/size/resizable=false
window/size/window_width_override=540
window/size/window_height_override=960
window/stretch/mode="viewport"
window/hdr_2d=false

[dotnet]

project/assembly_name="Tappie"

[rendering]

renderer/rendering_method="mobile"
